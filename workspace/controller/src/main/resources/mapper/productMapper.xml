<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.controller.mapper.ProductMapper">
    <insert id="insert">
        INSERT INTO TBL_PRODUCT
        VALUES (SEQ_PRODUCT.NEXTVAL, #{productName}, #{productBrand}, #{productPrice}, #{productStock})
    </insert>
</mapper>